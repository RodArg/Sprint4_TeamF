			{{#each transArr as |TransactionEle| }}
			date: {{ TransactionEle.date }}
			company: {{ TransactionEle.company }}
			amount: {{ TransactionEle.amount }}
			{{/each}}
		</table>
	</div>
	{{{ body }}}
</body>
</html>